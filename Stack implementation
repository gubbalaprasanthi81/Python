stack = []

while True:
    print("\n1.Push 2.Pop 3.Display 4.Exit")
    ch = int(input("Choice: "))

    if ch == 1:
        stack.append(input("Enter element: "))
    elif ch == 2:
        print("Popped:", stack.pop() if stack else "Empty")
    elif ch == 3:
        print(stack)
    else:
        break
