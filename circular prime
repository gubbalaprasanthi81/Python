def is_prime(n):
    if n < 2: return False
    for i in range(2, int(n**0.5)+1):
        if n % i == 0:
            return False
    return True

num = input("Enter number: ")

if all(is_prime(int(num[i:] + num[:i])) for i in range(len(num))):
    print("Circular Prime")
else:
    print("Not Circular Prime")
