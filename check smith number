def digit_sum(n):
    return sum(int(d) for d in str(n))

def prime_factors(n):
    factors = []
    i = 2
    while i * i <= n:
        while n % i == 0:
            factors.append(i)
            n //= i
        i += 1
    if n > 1:
        factors.append(n)
    return factors

num = int(input("Enter number: "))

if num > 1 and digit_sum(num) == sum(digit_sum(f) for f in prime_factors(num)):
    print("Smith Number")
else:
    print("Not Smith")
